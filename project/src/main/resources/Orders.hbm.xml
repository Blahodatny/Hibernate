<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="retail.Orders" table="orders" schema="public" catalog="Retail_Service">
        <id name="orderNumber">
            <column name="order_number" sql-type="integer"/>
        </id>
        <property name="tostreet">
            <column name="tostreet" sql-type="text"/>
        </property>
        <property name="tocity">
            <column name="tocity" sql-type="text"/>
        </property>
        <property name="shipdate">
            <column name="shipdate" sql-type="timestamp"/>
        </property>
        <many-to-one name="customersByPhone" class="retail.Customers">
            <column name="phone"/>
        </many-to-one>
    </class>
</hibernate-mapping>